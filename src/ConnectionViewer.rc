			MENUITEM "カラムの設定...", CM_INTERFACE_LIST_COLUMN_SETTINGS
			MENUITEM SEPARATOR
			MENUITEM "インデックス", CM_INTERFACECOLUMN_INTERFACE_INDEX
			MENUITEM "名称", CM_INTERFACECOLUMN_DESCRIPTION
			MENUITEM "別名", CM_INTERFACECOLUMN_ALIAS
			MENUITEM "LUID", CM_INTERFACECOLUMN_INTERFACE_LUID
			MENUITEM "GUID", CM_INTERFACECOLUMN_INTERFACE_GUID
			MENUITEM "物理アドレス", CM_INTERFACECOLUMN_PHYSICAL_ADDRESS
			MENUITEM "固有物理アドレス", CM_INTERFACECOLUMN_PERMANENT_PHYSICAL_ADDRESS
			MENUITEM "MTU", CM_INTERFACECOLUMN_MTU
			MENUITEM "種類", CM_INTERFACECOLUMN_TYPE
			MENUITEM "トンネル", CM_INTERFACECOLUMN_TUNNEL_TYPE
			MENUITEM "媒体", CM_INTERFACECOLUMN_MEDIA_TYPE
			MENUITEM "物理媒体", CM_INTERFACECOLUMN_PHYSICAL_MEDIUM_TYPE
			MENUITEM "アクセス", CM_INTERFACECOLUMN_ACCESS_TYPE
			MENUITEM "方向", CM_INTERFACECOLUMN_DIRECTION_TYPE
			MENUITEM "操作状態", CM_INTERFACECOLUMN_OPERATIONAL_STATUS
			MENUITEM "管理状態", CM_INTERFACECOLUMN_ADMINISTRATIVE_STATUS
			MENUITEM "接続状態", CM_INTERFACECOLUMN_MEDIA_CONNECTION_STATE
			MENUITEM "ネットワークGUID", CM_INTERFACECOLUMN_NETWORK_GUID
			MENUITEM "接続タイプ", CM_INTERFACECOLUMN_CONNECTION_TYPE
			MENUITEM "送信リンク速度", CM_INTERFACECOLUMN_TRANSMIT_LINK_SPEED
			MENUITEM "受信リンク速度", CM_INTERFACECOLUMN_RECEIVE_LINK_SPEED
			MENUITEM "受信量", CM_INTERFACECOLUMN_IN_OCTETS
			MENUITEM "受信速度", CM_INTERFACECOLUMN_IN_BANDWIDTH
			MENUITEM "最大受信速度", CM_INTERFACECOLUMN_MAX_IN_BANDWIDTH
			MENUITEM "ユニキャスト受信パケット", CM_INTERFACECOLUMN_IN_UNICAST_PACKETS
			MENUITEM "非ユニキャスト受信パケット", CM_INTERFACECOLUMN_IN_NON_UNICAST_PACKETS
			MENUITEM "破棄受信パケット", CM_INTERFACECOLUMN_IN_DISCARDED_PACKETS
			MENUITEM "エラー受信パケット", CM_INTERFACECOLUMN_IN_ERROR_PACKETS
			MENUITEM "不明プロトコル受信パケット", CM_INTERFACECOLUMN_IN_UNKNOWN_PROTOCOL_PACKETS
			MENUITEM "ユニキャスト受信量", CM_INTERFACECOLUMN_IN_UNICAST_OCTETS
			MENUITEM "マルチキャスト受信量", CM_INTERFACECOLUMN_IN_MULTICAST_OCTETS
			MENUITEM "ブロードキャスト受信量", CM_INTERFACECOLUMN_IN_BROADCAST_OCTETS
			MENUITEM "送信量", CM_INTERFACECOLUMN_OUT_OCTETS
			MENUITEM "送信速度", CM_INTERFACECOLUMN_OUT_BANDWIDTH
			MENUITEM "最大送信速度", CM_INTERFACECOLUMN_MAX_OUT_BANDWIDTH
			MENUITEM "ユニキャスト送信パケット", CM_INTERFACECOLUMN_OUT_UNICAST_PACKETS
			MENUITEM "非ユニキャスト送信パケット", CM_INTERFACECOLUMN_OUT_NON_UNICAST_PACKETS
			MENUITEM "破棄送信パケット", CM_INTERFACECOLUMN_OUT_DISCARDED_PACKETS
			MENUITEM "エラー送信パケット", CM_INTERFACECOLUMN_OUT_ERROR_PACKETS
			MENUITEM "ユニキャスト送信量", CM_INTERFACECOLUMN_OUT_UNICAST_OCTETS
			MENUITEM "マルチキャスト送信量", CM_INTERFACECOLUMN_OUT_MULTICAST_OCTETS
			MENUITEM "ブロードキャスト送信量", CM_INTERFACECOLUMN_OUT_BROADCAST_OCTESTS
		END
		POPUP "ブロック表示項目(&B)"
		BEGIN
			MENUITEM "カラムの設定...", CM_BLOCK_LIST_COLUMN_SETTINGS
			MENUITEM SEPARATOR
			MENUITEM "アドレス", CM_BLOCKLIST_COLUMN_REMOTE_ADDRESS
			MENUITEM "ホスト名", CM_BLOCKLIST_COLUMN_REMOTE_HOST
			MENUITEM "国", CM_BLOCKLIST_COLUMN_COUNTRY
			MENUITEM "地域", CM_BLOCKLIST_COLUMN_CITY
			MENUITEM "位置", CM_BLOCKLIST_COLUMN_LOCATION
			MENUITEM "追加日時", CM_BLOCKLIST_COLUMN_ADDED_TIME
			MENUITEM "コメント", CM_BLOCKLIST_COLUMN_COMMENT
		END
		MENUITEM SEPARATOR
		MENUITEM "ホスト名の逆引きを行う(&A)", CM_RESOLVE_ADDRESSES
		POPUP "接続状況表示対象(&N)"
		BEGIN
			MENUITEM "TCP(IPv4)", CM_CONNECTIONLIST_PROTOCOL_TCP_V4
			MENUITEM "TCP(IPv6)", CM_CONNECTIONLIST_PROTOCOL_TCP_V6
			MENUITEM "UDP(IPv4)", CM_CONNECTIONLIST_PROTOCOL_UDP_V4
			MENUITEM "UDP(IPv6)", CM_CONNECTIONLIST_PROTOCOL_UDP_V6
		END
