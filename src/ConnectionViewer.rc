			MENUITEM "送信量", CM_LISTCOLUMN_OUT_BYTES
			MENUITEM "パス", CM_LISTCOLUMN_PROCESS_PATH
		END
		POPUP "接続ログ表示項目(&L)"
		BEGIN
			MENUITEM "カラムの設定...", CM_CONNECTION_LOG_COLUMN_SETTINGS
			MENUITEM SEPARATOR
			MENUITEM "日時", CM_LOGCOLUMN_CREATE_TIME
			MENUITEM "更新日時", CM_LOGCOLUMN_UPDATE_TIME
			MENUITEM "プロセス", CM_LOGCOLUMN_PROCESS_NAME
			MENUITEM "PID", CM_LOGCOLUMN_PROCESS_ID
			MENUITEM "プロトコル", CM_LOGCOLUMN_PROTOCOL
			MENUITEM "ローカル アドレス", CM_LOGCOLUMN_LOCAL_ADDRESS
			MENUITEM "ローカル ポート", CM_LOGCOLUMN_LOCAL_PORT
			MENUITEM "リモート アドレス", CM_LOGCOLUMN_REMOTE_ADDRESS
			MENUITEM "リモート ポート", CM_LOGCOLUMN_REMOTE_PORT
			MENUITEM "ホスト名", CM_LOGCOLUMN_REMOTE_HOST
			MENUITEM "国", CM_LOGCOLUMN_COUNTRY
			MENUITEM "地域", CM_LOGCOLUMN_CITY
			MENUITEM "位置", CM_LOGCOLUMN_LOCATION
			MENUITEM "状態", CM_LOGCOLUMN_STATE
			MENUITEM "接続時間", CM_LOGCOLUMN_TIME
