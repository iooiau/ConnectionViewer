IDM_MAIN MENU
BEGIN
	POPUP "ファイル(&F)"
	BEGIN
		MENUITEM "リストを保存(&S)...\tCtrl+S", CM_SAVE_LIST
		MENUITEM SEPARATOR
		MENUITEM "終了(&X)\tAlt+F4", CM_EXIT
	END
	POPUP "編集(&E)"
	BEGIN
		MENUITEM "リストをコピー(&C)\tCtrl+C", CM_COPY_LIST
		MENUITEM SEPARATOR
		MENUITEM "ログをクリア(&L)", CM_LOG_CLEAR
	END
	POPUP "表示(&V)"
	BEGIN
		MENUITEM "更新停止(&P)\tPause", CM_PAUSE
		POPUP "更新間隔(&U)"
		BEGIN
			MENUITEM "0.5秒", CM_UPDATEINTERVAL_500
			MENUITEM "1秒", CM_UPDATEINTERVAL_1000
			MENUITEM "2秒", CM_UPDATEINTERVAL_2000
			MENUITEM "3秒", CM_UPDATEINTERVAL_3000
			MENUITEM "5秒", CM_UPDATEINTERVAL_5000
		END
		MENUITEM SEPARATOR
		POPUP "接続状況表示項目(&C)"
		BEGIN
			MENUITEM "カラムの設定...", CM_CONNECTION_LIST_COLUMN_SETTINGS
			MENUITEM SEPARATOR
			MENUITEM "プロセス", CM_LISTCOLUMN_PROCESS_NAME
			MENUITEM "PID", CM_LISTCOLUMN_PROCESS_ID
			MENUITEM "プロトコル", CM_LISTCOLUMN_PROTOCOL
			MENUITEM "ローカル アドレス", CM_LISTCOLUMN_LOCAL_ADDRESS
			MENUITEM "ローカル ポート", CM_LISTCOLUMN_LOCAL_PORT
			MENUITEM "リモート アドレス", CM_LISTCOLUMN_REMOTE_ADDRESS
			MENUITEM "リモート ポート", CM_LISTCOLUMN_REMOTE_PORT
			MENUITEM "ホスト名", CM_LISTCOLUMN_REMOTE_HOST
			MENUITEM "国", CM_LISTCOLUMN_COUNTRY
			MENUITEM "地域", CM_LISTCOLUMN_CITY
			MENUITEM "位置", CM_LISTCOLUMN_LOCATION
			MENUITEM "状態", CM_LISTCOLUMN_STATE
			MENUITEM "接続時間", CM_LISTCOLUMN_TIME
			MENUITEM "受信速度", CM_LISTCOLUMN_IN_BANDWIDTH
